e548e61c74b6891d64999a4351246792|file:///home/jdv/.zshrc|205|1|V|zsh
	# Recurse replaces 4 leading spaces with a tab
	space4tab() {
		"find . -name '*.$1' ! -type d -exec bash -c 'expand -t 4 "$0" > /tmp/e && mv /tmp/e "$0"' {} \;"
	}
