	    function shrinkBar() {
	        console.log("Shrinking Bar!");
	      var time = document.getElementById("time");   
	      var width = 60000;
	      var id = setInterval(frame, 1);
	      function frame() {
	        if (width <= 0) {
	          clearInterval(id);
	          time.style.width = '0%'; 
	        } else {
	          width--; 
	          time.style.width = (width/60) + '%'; 
	        }
	      }
	    }
	
